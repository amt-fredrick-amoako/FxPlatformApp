@model Dictionary<string, object>

<img src="@Model["logo"]" alt="200px" />
<h1>@Model["name"] (@Model["ticker"])</h1>
<span class="pill">@Model["finnhubIndustry"]</span>

<h3 class="mb">Exchange: <span class="pill">@Model["exchange"]</span></h3>

<h1 class="stock-price">
    <span class="text-grey">$</span>
    @Model["price"]
</h1>

<a id="trade-button" href="~/Trade/Index/@Model["ticker"]" class="button-link button-blue-back">Trade Now</a>